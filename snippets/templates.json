    "Sticky-Footer": {
        "prefix": "bscf-template-sticky-footer",
        "body": [
            "<!doctype html>",
            "<html lang=\"en\" class=\"h-100\">",
            "$0",
            "<head>",
            "    <title>${1:PageTitle}</title>",
